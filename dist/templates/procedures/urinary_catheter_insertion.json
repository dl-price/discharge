{
  "id": "urinary_catheter_insertion",
  "title": "Urinary catheter insertion",
  "category": "Procedures",
  "version": "1.0",
  "lastReviewed": "2025-12-20",
  "keywords": [
    "IDC",
    "catheter",
    "urinary",
    "Foley"
  ],
  "fields": [
    {
      "name": "date",
      "label": "Date",
      "type": "date",
      "required": false
    },
    {
      "name": "time",
      "label": "Time",
      "type": "text",
      "required": false
    },
    {
      "name": "indication",
      "label": "Indication",
      "type": "textarea",
      "required": true
    },
    {
      "name": "catheter_type",
      "label": "Catheter type",
      "type": "select",
      "required": false,
      "options": [
        "IDC",
        "Foley",
        "Other"
      ]
    },
    {
      "name": "catheter_type_other",
      "label": "Catheter type (other)",
      "type": "text",
      "required": false
    },
    {
      "name": "size",
      "label": "Catheter size",
      "type": "select",
      "required": false,
      "options": [
        "12 Fr",
        "14 Fr",
        "16 Fr",
        "18 Fr",
        "20 Fr"
      ]
    },
    {
      "name": "balloon_ml",
      "label": "Balloon volume (mL)",
      "type": "number",
      "required": false
    },
    {
      "name": "balloon_sterile_water",
      "label": "Balloon filled with sterile water",
      "type": "checkbox",
      "required": false,
      "default": false
    },
    {
      "name": "aseptic",
      "label": "Aseptic technique used",
      "type": "checkbox",
      "required": false,
      "default": false
    },
    {
      "name": "foreskin_replaced",
      "label": "Foreskin replaced",
      "type": "checkbox",
      "required": false,
      "default": false
    },
    {
      "name": "urine_return",
      "label": "Free urine return",
      "type": "checkbox",
      "required": false,
      "default": false
    },
    {
      "name": "urine_description",
      "label": "Urine appearance",
      "type": "text",
      "required": false
    },
    {
      "name": "comp_none",
      "label": "Complications: None",
      "type": "checkbox",
      "required": false,
      "default": false
    },
    {
      "name": "comp_trauma",
      "label": "Complications: Traumatic insertion",
      "type": "checkbox",
      "required": false,
      "default": false
    },
    {
      "name": "comp_haematuria",
      "label": "Complications: Haematuria",
      "type": "checkbox",
      "required": false,
      "default": false
    },
    {
      "name": "comp_failed",
      "label": "Complications: Failed attempt",
      "type": "checkbox",
      "required": false,
      "default": false
    },
    {
      "name": "comp_other",
      "label": "Complications: Other",
      "type": "text",
      "required": false
    },
    {
      "name": "tolerated",
      "label": "Patient tolerated procedure",
      "type": "checkbox",
      "required": false,
      "default": false
    },
    {
      "name": "plan_monitor",
      "label": "Plan: Monitor urine output and catheter site",
      "type": "checkbox",
      "required": false,
      "default": false
    }
  ],
  "reviewStatus": "alpha",
  "body": "Urinary catheter insertion note\nDate: {{date}}\nTime: {{time}}\nIndication: {{indication}}\nCatheter type: {{catheter_type}}{{#if catheter_type_other}} ({{catheter_type_other}}){{/if}}\nCatheter size: {{size}}\n{{#if balloon_ml}}Balloon volume: {{balloon_ml}} mL\n{{/if}}\n{{#if balloon_sterile_water}}- Balloon filled with sterile water\n{{/if}}\nProcedure:\n{{#if aseptic}}- Aseptic technique used\n{{/if}}- Catheter inserted using standard technique\n{{#if foreskin_replaced}}- Foreskin replaced\n{{/if}}\n\nOutcome:\n{{#if urine_return}}- Free urine return\n{{/if}}{{#if urine_description}}- Urine appearance: {{urine_description}}\n{{/if}}\nComplications:\n{{#if comp_none}}- None\n{{/if}}{{#if comp_trauma}}- Traumatic insertion\n{{/if}}{{#if comp_haematuria}}- Haematuria\n{{/if}}{{#if comp_failed}}- Failed attempt\n{{/if}}{{#if comp_other}}- Other: {{comp_other}}\n{{/if}}\n{{#if tolerated}}Patient tolerated procedure.\n{{/if}}\nPlan:\n{{#if plan_monitor}}- Monitor urine output and catheter site\n{{/if}}"
}
